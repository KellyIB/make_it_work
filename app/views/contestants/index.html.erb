
<center>
<h1>Contestant Index Page</h1>
<% @contestants.each do |contestant| %>
  <section id="contestant-<%=contestant.id%>">
    <p> Name: <%= contestant.name %> </p>
    <p> Age: <%= contestant.age %> </p>
    <p> Hometown: <%= contestant.hometown %> </p>
    <p> Years of Experience: <%= contestant.years_of_experience %> </p>
    <p> <% contestant.projects.map do |project|%> </p>
        <p> Project: <%= project.name %> </p>
    <% end %>
  </section>
  <br></br>
<% end %>
</center>
